-- anima√ß√£o de abrir
local function abrirJanela()
	clickSound:Play()
	frame.Visible = true
	blurFrame.Visible = true
	frame.Position = UDim2.new(0.5, -190, 1, 20)
	TweenService:Create(frame, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
		Position = UDim2.new(0.5, -190, 0.5, -190)
	}):Play()
end

-- anima√ß√£o de fechar
local function fecharJanela()
	clickSound:Play()
	local tween = TweenService:Create(frame, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.In), {
		Position = UDim2.new(0.5, -190, 1, 20)
	})
	tween:Play()
	tween.Completed:Connect(function()
		frame.Visible = false
		blurFrame.Visible = false
	end)
end

-- verifica√ß√µes
local function verificarArquivo(txt)
	if txt == "" then
		return false, "‚ö†Ô∏è Nenhum conte√∫do detectado!"
	end
	if string.find(txt, "{") and string.find(txt, "}") then
		return true, "‚úÖ JSON v√°lido!"
	elseif string.find(txt, "function") or string.find(txt, "return") then
		return true, "‚úÖ Script Lua compat√≠vel!"
	elseif string.find(txt, "Keyframes") or string.find(txt, "AnimationId") then
		return true, "‚úÖ Arquivo de anima√ß√£o detectado!"
	else
		return false, "‚ùå Tipo desconhecido!"
	end
end

-- conectar bot√µes
openButton.MouseButton1Click:Connect(abrirJanela)
closeButton.MouseButton1Click:Connect(fecharJanela)

importButton.MouseButton1Click:Connect(function()
	clickSound:Play()
	local ok, msg = verificarArquivo(inputBox.Text)
	statusLabel.Text = msg
	statusLabel.TextColor3 = ok and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 50, 50)
end)

saveButton.MouseButton1Click:Connect(function()
	clickSound:Play()
	if inputBox.Text == "" then
		statusLabel.Text = "‚ö†Ô∏è Nada para salvar!"
		statusLabel.TextColor3 = Color3.fromRGB(255, 200, 0)
		return
	end
	local module = Instance.new("ModuleScript")
	module.Name = "Importado_" .. math.random(1000, 9999)
	module.Source = inputBox.Text
	module.Parent = game.ReplicatedStorage
	statusLabel.Text = "üíæ Arquivo salvo em ReplicatedStorage!"
	statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
end)

