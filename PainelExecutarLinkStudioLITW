local tweenIn = TweenService:Create(frame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(0, 480, 0, 320), BackgroundTransparency = 0})
local tweenOut = TweenService:Create(frame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {BackgroundTransparency = 1})

toggle.MouseButton1Click:Connect(function()
	aberto = not aberto
	if aberto then
		frame.Visible = true
		frame.BackgroundTransparency = 1
		tweenIn:Play()
		toggle.Text = "‚ùå Fechar"
	else
		tweenOut:Play()
		task.wait(0.4)
		frame.Visible = false
		toggle.Text = "üìÅ Importador"
	end
end)

-- Fun√ß√£o importar
importBtn.MouseButton1Click:Connect(function()
	local url = urlBox.Text
	if url == "" then
		warn("[Importador] Nenhuma URL inserida.")
		return
	end
	local ok, result = pcall(function()
		return game:HttpGet(url)
	end)
	if ok then
		inputBox.Text = result
		print("[Studio Lite] Arquivo importado com sucesso!")
	else
		warn("[Importador] Falha ao baixar o arquivo. Verifique a URL (precisa ser RAW).")
	end
end)

-- Fun√ß√£o salvar (simula download)
saveBtn.MouseButton1Click:Connect(function()
	local conteudo = inputBox.Text
	if conteudo == "" then
		warn("[Importador] Nenhum conte√∫do para salvar.")
		return
	end

	-- Aqui simula o salvamento (pode ser adaptado para enviar a servidor real)
	print("[Studio Lite] Arquivo salvo com sucesso!")
	print(conteudo:sub(1, 200) .. "...")
end)dButton.TextSize = 16
